{
    "name": "hb-report",
    "version": "0.0.2",
    "type": "module",
    "description": "A cross-platform project reporting tool for Project data",
    "main": "src/main/main.js",
    "scripts": {
        "build": "webpack --config webpack.config.js",
        "build:main": "xcopy src\\main dist\\main /E /I /Y || robocopy src\\main dist\\main /E /MIR",
        "dev": "cross-env NODE_ENV=development webpack --config webpack.config.js && cross-env NODE_ENV=development electron .",
        "prestart:main": "webpack --mode development",
        "package": "electron-builder",
        "start": "cross-env NODE_ENV=production electron .",
        "start:main": "nodemon --exec electron .",
        "start:renderer": "webpack serve --config webpack.config.js",
        "test": "node scripts/runTests.js",
        "test:raw": "jest"
    },
    "jest": {
        "transform": {
            "^.+\\.(js|jsx)$": "babel-jest"
        },
        "testEnvironment": "jsdom",
        "moduleFileExtensions": [
            "js",
            "jsx"
        ],
        "moduleNameMapper": {
            "\\.(css|less|scss|sass)$": "identity-obj-proxy"
        },
        "setupFilesAfterEnv": [
            "<rootDir>/jest.setup.js"
        ]
    },
    "author": "Bobby Fetting",
    "license": "MIT",
    "dependencies": {
        "@ag-grid-community/client-side-row-model": "^32.3.4",
        "@ag-grid-community/core": "^32.3.4",
        "@ant-design/icons": "^5.6.1",
        "@ant-design/v5-patch-for-react-19": "^1.0.3",
        "@hookform/resolvers": "^5.0.1",
        "@procore/js-sdk": "^4.2.1",
        "@reduxjs/toolkit": "^2.6.1",
        "@tanstack/react-table": "^8.21.2",
        "ag-charts-enterprise": "^11.2.1",
        "ag-charts-react": "^11.2.1",
        "ag-grid-community": "^33.1.1",
        "ag-grid-enterprise": "^33.2.1",
        "ag-grid-react": "^33.2.1",
        "antd": "^5.24.4",
        "antd-dayjs-webpack-plugin": "^1.0.6",
        "axios": "^1.8.4",
        "bcrypt": "^5.1.1",
        "chart.js": "^4.4.8",
        "chartjs-adapter-date-fns": "^3.0.0",
        "chartjs-adapter-moment": "^1.0.1",
        "chartjs-plugin-annotation": "^3.1.0",
        "chartjs-plugin-datalabels": "^2.2.0",
        "chartjs-plugin-zoom": "^2.2.0",
        "date-fns": "^4.1.0",
        "dayjs": "^1.11.13",
        "electron": "^35.0.2",
        "electron-log": "^5.3.2",
        "electron-store": "^10.0.1",
        "html2canvas": "^1.4.1",
        "moment": "^2.30.1",
        "node-cron": "^3.0.3",
        "node-fetch": "^3.3.2",
        "nodemailer": "^6.10.0",
        "pdfkit": "^0.16.0",
        "pg": "^8.14.1",
        "pg-hstore": "^2.3.4",
        "prop-types": "^15.8.1",
        "react": "^19.0.0",
        "react-chartjs-2": "^5.3.0",
        "react-currency-input-field": "^3.10.0",
        "react-dom": "^19.0.0",
        "react-google-charts": "^5.2.1",
        "react-grid-layout": "^1.5.1",
        "react-hook-form": "^7.54.2",
        "react-number-format": "^5.4.3",
        "react-redux": "^9.2.0",
        "reselect": "^5.1.1",
        "sequelize": "^6.37.7",
        "slate": "^0.112.0",
        "slate-react": "^0.112.1",
        "sqlite3": "^5.1.7",
        "uuid": "^11.1.0",
        "winston": "^3.17.0",
        "winston-daily-rotate-file": "^5.0.0",
        "yup": "^1.6.1"
    },
    "devDependencies": {
        "@babel/core": "^7.26.10",
        "@babel/preset-env": "^7.26.9",
        "@babel/preset-react": "^7.26.3",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.2.0",
        "babel-jest": "^29.7.0",
        "babel-loader": "^10.0.0",
        "concurrently": "^9.1.2",
        "copy-webpack-plugin": "^13.0.0",
        "cpr": "^3.0.1",
        "cross-env": "^7.0.3",
        "css-loader": "^7.1.2",
        "dree": "^5.1.5",
        "electron-builder": "^25.1.8",
        "html-webpack-plugin": "^5.6.3",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jwt-decode": "^4.0.0",
        "nodemon": "^3.1.9",
        "style-loader": "^4.0.0",
        "webpack": "^5.98.0",
        "webpack-cli": "^6.0.1",
        "webpack-dev-server": "^5.2.0"
    },
    "build": {
        "appId": "com.hbreport.app",
        "productName": "HB Report",
        "files": [
            "dist/**/*",
            "node_modules/**/*"
        ],
        "directories": {
            "output": "release"
        },
        "win": {
            "target": "nsis"
        },
        "mac": {
            "target": "dmg"
        },
        "linux": {
            "target": "AppImage"
        }
    }
}
